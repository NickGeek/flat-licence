{
	"name": "flat-licence",
	"version": "0.1.1",
	"description": "Generate a markdown list of all included prod works and their licences from a package.json",
	"main": "dist/index.js",
	"scripts": {
		"start": "node dist/index.js",
		"build": "npx tsc",
		"build_run": "npm run build; npm run start"
	},
	"preferGlobal": true,
	"directories": {
		"bin": "./bin"
	},
	"keywords": [
		"licence",
		"lisense",
		"markdown"
	],
	"author": "Nick Webster",
	"license": "MPL-2.0",
	"devDependencies": {
		"@types/get-stdin": "^5.0.1",
		"@types/license-checker": "^15.0.0",
		"@types/node": "^10.3.2",
		"typescript": "^2.9.1"
	},
	"dependencies": {
		"get-stdin": "^6.0.0",
		"license-checker": "^20.1.0"
	}
}
